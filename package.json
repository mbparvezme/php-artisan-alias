{
  "name": "php-artisan-alias",
  "displayName": "PHP Artisan Alias",
  "description": "Set a custom alias for the 'php artisan' command.",
  "version": "0.9.0",
  "publisher": "mbparvezme",
  "publisherUrl": "https://www.mbparvez.me",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.0.0"
  },
  "repository": "https://github.com/mbparvezme/php-artisan-alias",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.setAlias",
    "onTerminalOpen"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.setAlias",
        "title": "Set PHP Artisan Alias"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "PHP Artisan Alias Configuration",
      "properties": {
        "phpArtisanAlias.alias": {
          "type": "string",
          "default": "art",
          "description": "Alias for 'php artisan' command"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode development",
    "watch": "webpack --watch",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/node": "^14.18.63",
    "ts-loader": "^9.5.1",
    "typescript": "^4.3.5",
    "vscode": "^1.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
